[pidora-updates-testing]
name=pidora $releasever - $basearch
#name=Fedora $releasever - $basearch
failovermethod=priority
mirrorlist=http://pidora.ca/mirrorlist/mirrorlist.cgi?repo=pidora-updates-testing-$releasever&arch=$basearch
enabled=0
metadata_expire=7d
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-pidora-18
protect=0

